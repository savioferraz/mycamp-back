    "prisma:generate-client": "npx prisma generate",
    "prisma:migration-generate": "npm run dev:load-envs prisma migrate dev",
    "prisma:migration-run": "prisma migrate deploy",
    "prisma:seed": "npm run dev:load-envs prisma db seed",
    "start": "npx nodemon src/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
