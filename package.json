  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "ttsc -p tsconfig.build.json",
    "db:dump": "sudo su -c 'pg_dump mycamp --inserts --no-owner' postgres > dump.sql",
    "dev:load-envs": "dotenv -e .env.development",
    "lint": "eslint .",
    "lint:staged": "lint-staged",
